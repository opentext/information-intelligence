		{
			"name": "Authentication",
			"item": [
				{
					"name": "Get PAAS Token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Validate response', function () {\r",
									"    pm.expect(pm.response.code).to.eql(200);\r",
									"})\r",
									"let otif_token = pm.response.headers.get('Set-Cookie').split(';')\r",
									"postman.setEnvironmentVariable(\"tmp_access_token\", otif_token[0])\r",
									"console.log(postman.getEnvironmentVariable(\"tmp_access_token\"))"
								],
								"type": "text/javascript"
							}
						}
					],
					"protocolProfileBehavior": {
						"disabledSystemHeaders": {
							"content-type": true
						}
					},
					"request": {
						"auth": {
							"type": "basic",
							"basic": [
								{
									"key": "password",
									"value": "admin",
									"type": "string"
								},
								{
									"key": "username",
									"value": "admin",
									"type": "string"
								}
							]
						},
						"method": "POST",
						"header": [],
						"url": {
							"raw": "https://{{document-api_url}}/mtm-gateway-api/services/security/authentication/login",
							"protocol": "https",
							"host": [
								"{{document-api_url}}"
							],
							"path": [
								"mtm-gateway-api",
								"services",
								"security",
								"authentication",
								"login"
							]
						}
					},
					"response": []
				},
				{
					"name": "Get OCP Token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Validate response', function () {\r",
									"    pm.expect(pm.response.code).to.eql(200);\r",
									"})\r",
									"if (responseBody.has(\"access_token\")) {\r",
									"    var jsonData = JSON.parse(responseBody);\r",
									"    postman.setEnvironmentVariable(\"tmp_access_token\", jsonData.access_token);\r",
									"    console.log(postman.getEnvironmentVariable(\"tmp_access_token\"));\r",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [],
						"body": {
							"mode": "urlencoded",
							"urlencoded": [
								{
									"key": "grant_type",
									"value": "{{grant_type}}",
									"type": "text"
								},
								{
									"key": "client_id",
									"value": "{{client_id}}",
									"type": "text"
								},
								{
									"key": "client_secret",
									"value": "{{client_secret}}",
									"type": "text"
								},
								{
									"key": "username",
									"value": "{{username}}",
									"type": "text"
								},
								{
									"key": "password",
									"value": "{{password}}",
									"type": "text"
								}
							]
						},
						"url": {
